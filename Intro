public class main{
    public static void main(String[] args) {
        int i = 0;
        while (i<5) {
            System.out.println(i);
            i++;           
        }

    }
}


public class main{
    public static void main(String[] args) {
        int a = 1;
        do {
            System.out.println(a);
            a++;
        }
        while(a<5);
    }
}


